<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.jeet.com/api"
	xmlns:tns="http://www.jeet.com/api" elementFormDefault="qualified">

	<element name="item">
		<complexType>
			<sequence>
				<element name="itemId" type="int" />
				<element name="itemName" type="string" />
				<element name="description" type="string" />
				<element name="sprice" type="int" />
				<element ref="tns:type"/>
			</sequence>
		</complexType>
	</element>

	<element name="type">
		<complexType>
			<sequence>
				<element name="typeId" type="int"/>
				<element name="name" type="string"/>
				<element ref="tns:unit"/>
			</sequence>
		</complexType>
	</element>
	
	<element name="unit">
		<complexType>
			<sequence>
				<element name="unitId" type="int"/>
				<element name="name" type="string"/>
			</sequence>
		</complexType>
	</element>
	
	<element name="offer">
		<complexType>
			<sequence>
				<element name="offerid" type="int"/>
				<element name="offerDetail" type="string"/>
				<element name="quantity" type="int"/>
				<element name="discount" type="int"/>
			</sequence>
		</complexType>
	</element>

<element name="supplier">
		<complexType>
			<sequence>
				<element name="supplierId" type="int"/>
				<element name="supplierName" type="string"/>
				<element name="quantity" type="int"/>
				<element ref="tns:item" maxOccurs="unbounded"/>
			</sequence>
		</complexType>
	</element>
	
